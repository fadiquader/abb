{
  "name": "@abb/server",
  "version": "1.0.0",
  "description": "Awesome project developed with TypeORM.",
  "scripts": {
    "start": "NODE_ENV=development nodemon --exec ts-node --files src/index.ts",
    "test": "NODE_ENV=test jest --watch",
    "build": "rimraf ./dist && tsc && copyfiles -u 1 src/**/*.graphql dist",
    "gen-schema-types": "ts-node src/scripts/createTypes.ts"
  },
  "jest": {
    "globalSetup": "./src/testUtils/callSetup.js",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "@gql2ts/from-query": "^1.9.0",
    "@gql2ts/from-schema": "^1.10.1",
    "@types/aws-lambda": "^8.10.14",
    "axios": "^0.18.0",
    "bcryptjs": "^2.4.3",
    "connect-redis": "^3.4.0",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "express-rate-limit": "^3.2.1",
    "express-session": "^1.15.6",
    "faker": "^4.1.0",
    "glob": "^7.1.3",
    "graphql": "^14.0.2",
    "graphql-import": "^0.7.1",
    "graphql-tools": "^4.0.2",
    "graphql-yoga": "^1.16.7",
    "ioredis": "^4.2.0",
    "merge-graphql-schemas": "^1.5.7",
    "node-fetch": "^2.2.0",
    "pg": "^7.5.0",
    "rate-limit-redis": "^1.5.0",
    "request-promise": "^4.2.2",
    "sparkpost": "^2.1.2",
    "typeorm": "^0.2.8",
    "uuid": "^3.3.2",
    "yup": "^0.26.6"
  },
  "devDependencies": {
    "@gql2ts/types": "^1.9.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/connect-redis": "^0.0.7",
    "@types/express-rate-limit": "^2.9.3",
    "@types/express-session": "^1.15.11",
    "@types/faker": "^4.1.4",
    "@types/glob": "^7.1.1",
    "@types/ioredis": "^4.0.3",
    "@types/jest": "^23.3.5",
    "@types/node": "^10.12.0",
    "@types/node-fetch": "^2.1.2",
    "@types/request-promise": "^4.1.42",
    "@types/sparkpost": "^2.1.4",
    "@types/uuid": "^3.4.4",
    "@types/ws": "^6.0.1",
    "@types/yup": "^0.26.0",
    "copyfiles": "^2.1.0",
    "dotenv-safe": "^6.1.0",
    "jest": "^23.6.0",
    "nodemon": "^1.18.4",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.10.4",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^3.1.3"
  }
}
